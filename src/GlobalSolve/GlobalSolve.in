environment
	tabular_data
		tabular_data_file "tabular.data"
	output_file "dakota.out"
	error_file "dakota.err"
	write_restart "dakota.rst"

method
	id_method "method1"
	soga
		model_pointer "model1"

model
	id_model "model1"
	single
		interface_pointer "interface1"
	variables_pointer "variables1"

variables
	id_variables "variables1"
	continuous_design 2
		initial_point 5.0 5.0
		lower_bounds -10.0 -10.0
		upper_bounds 10.0 10.0
		descriptors "x1" "y1"

interface
	id_interface "interface1"
	analysis_drivers "python -m optim"
		fork
			parameters_file "params.in"
			results_file "results.out"
			work_directory
				named "workdir/run"
				directory_tag
				directory_save
				copy_files "runCFS.py" "myTemplate.template" "qoi.py" "optim.py"
	asynchronous
		evaluation_concurrency 2

responses
	id_responses "responses1"
	objective_functions=1
	descriptors "frequency"
	no_gradients
	no_hessians
