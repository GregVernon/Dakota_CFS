environment
	tabular_data
	tabular_data_file "tabular.data"
	output_file "dakota.out"
	error_file "dakota.err"
	write_restart "dakota.rst"

method
	id_method "method1"
	optpp_q_newton
		model_pointer "model1"

model
	id_model "model1"
	single
		interface_pointer "interface1"
	variables_pointer "variables1"
	responses_pointer "responses1"

variables
	id_variables "variables1"
	continuous_design 4
		initial_point +1.00 +1.00 +0.25 +0.25
		descriptors   "x1" "y1" "x2" "y2"

interface
	id_interface "interface1"
	analysis_drivers "python3 -m optim"
		fork
			parameters_file "params.in"
			results_file "results.out"
			work_directory
				named "workdir/run"
				directory_tag
				directory_save
				copy_files "runCFS.py" "circleGeom.trelis" "myTemplate.template" "qoi.py" "optim.py"
	asynchronous
		evaluation_concurrency 10

responses
	id_responses "responses1"
	descriptors "frequency" "nlcon_1" "nlcon_2"
	objective_functions = 1
	nonlinear_inequality_constraints = 2
	numerical_gradients
		method_source dakota
		interval_type central
		fd_step_size = 1.e-4
	no_hessians
